{
  "tasks": [
    {
      "type": "shell",
      "label": "g++-12 build active file",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "/usr/bin/g++-12",
      "args": ["-g", "-O0", "-std=c++20", "${file}"],
      "problemMatcher": ["$gcc"]
    }
  ],
  "version": "2.0.0"
}
